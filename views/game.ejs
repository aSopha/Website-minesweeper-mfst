<!doctype html>
<html lang="en">
<head>
	<title>MF&ST</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
	<link rel="stylesheet" href="./PhaserGame/style.css">
	<link rel="icon" href="./PhaserGame/Assets/papi.png" </head>
</head>
<body>
	<div id="game-and-scores" class="gamestuff">
		<div id="my-game" class="gamestuff">
		</div>
		<div id="scores">
			<div id="title">
				High Scores
			</div>
		    <ol>
		      <% for(var i=0; i<10; i++) {%>
				<% if(i==scores.length) break;%>
		        <li class="score">
		          <span><%= scores[i].name %></span>
		          <span><%= scores[i].score %></span>
		        </li>
		      <% } %>
		  </ol>
		</div>
	</div>
	<h1> WASD to move </br>
		Mouse to aim </br>
		Left Click to shoot</br>
		Minimum Score for High Score
		<div id="lowest"><%= scores[9].score %>
		</div>
	</h1>
	<script type="text/javascript" src="./PhaserGame/js/phaser.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/Weapon.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/Baddies.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/Level.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/Player.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/boot.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/load.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/menu.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/play.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/shop.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/gameOver.js"></script>
	<script type="text/javascript" src="./PhaserGame/js/win.js"></script>
    <script type="text/javascript" src="./PhaserGame/js/game.js"></script>
	<script src="https://use.fontawesome.com/9e1d26a78c.js"></script>
	<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
	</body>
</html>
